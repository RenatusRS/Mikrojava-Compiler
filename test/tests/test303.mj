	{
		p.ispisi();
		print(eol); 
		print(p.dohvTezinu());
	}
		
	void main() Predmet predmeti[]; int i; Sfera s1, s2, s3; Kvadar k1, k2, k3; int t; Displayable d1, d2;
	{
		
		predmeti = new Predmet[6];
